#include <iostream>
#include <cmath>

using namespace std;

double f(double x){
    return log(x)/x;
}

double Right_Rect(double a, double b, double n){
    double h = (b - a) / n;
    double sum;
    for(int i = 1; i <= n; i++){
        sum+=h*f(a+i*h);
    }
    return sum;
}

int main(){
    int n;
    cout << "n: ";
    cin >> n;
    cout << Right_Rect(1,2,n);
    return 0;
